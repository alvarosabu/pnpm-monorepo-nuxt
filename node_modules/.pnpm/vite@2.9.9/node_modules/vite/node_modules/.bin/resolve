#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/resolve@1.22.0/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite@2.9.9/node_modules/vite/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite@2.9.9/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/resolve@1.22.0/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite@2.9.9/node_modules/vite/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite@2.9.9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
