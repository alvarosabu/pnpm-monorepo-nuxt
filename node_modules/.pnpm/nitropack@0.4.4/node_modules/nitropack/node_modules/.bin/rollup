#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules/rollup/dist/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules/rollup/dist/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules/rollup/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/nitropack@0.4.4/node_modules/nitropack/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/nitropack@0.4.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules/rollup/dist/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules/rollup/dist/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules/rollup/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/rollup@2.74.1/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/nitropack@0.4.4/node_modules/nitropack/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/nitropack@0.4.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.74.1/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@2.74.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
