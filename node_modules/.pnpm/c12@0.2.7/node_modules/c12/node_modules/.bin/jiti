#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/jiti@1.13.0/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/c12@0.2.7/node_modules/c12/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/c12@0.2.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/jiti@1.13.0/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/c12@0.2.7/node_modules/c12/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/c12@0.2.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jiti@1.13.0/node_modules/jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../../../../../jiti@1.13.0/node_modules/jiti/bin/jiti.js" "$@"
fi
