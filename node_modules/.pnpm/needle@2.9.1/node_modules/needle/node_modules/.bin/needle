#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/needle@2.9.1/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/needle@2.9.1/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/needle" "$@"
else
  exec node  "$basedir/../../bin/needle" "$@"
fi
