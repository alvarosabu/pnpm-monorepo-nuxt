    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.14/node_modules/autoprefixer/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.14/node_modules/autoprefixer/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.14/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/@nuxt+vite-builder@3.0.0-rc.3_vue@3.2.36/node_modules/@nuxt/vite-builder/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/@nuxt+vite-builder@3.0.0-rc.3_vue@3.2.36/node_modules/@nuxt/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/@nuxt+vite-builder@3.0.0-rc.3_vue@3.2.36/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.14/node_modules/autoprefixer/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.14/node_modules/autoprefixer/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.14/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/@nuxt+vite-builder@3.0.0-rc.3_vue@3.2.36/node_modules/@nuxt/vite-builder/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/@nuxt+vite-builder@3.0.0-rc.3_vue@3.2.36/node_modules/@nuxt/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/@nuxt+vite-builder@3.0.0-rc.3_vue@3.2.36/node_modules"
