#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite-plugin-checker@0.4.6_vite@2.9.9/node_modules/vite-plugin-checker/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite-plugin-checker@0.4.6_vite@2.9.9/node_modules/vite-plugin-checker/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite-plugin-checker@0.4.6_vite@2.9.9/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/vite-plugin-checker/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/vite-plugin-checker/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite-plugin-checker@0.4.6_vite@2.9.9/node_modules/vite-plugin-checker/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite-plugin-checker@0.4.6_vite@2.9.9/node_modules/vite-plugin-checker/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/vite-plugin-checker@0.4.6_vite@2.9.9/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/.pnpm/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules:/Users/x400048/Projects/github/node_modules:/Users/x400048/Projects/node_modules:/Users/x400048/node_modules:/Users/node_modules:/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/vite-plugin-checker/bin/node_modules:/Users/x400048/Projects/github/pnpm-monorepo-nuxt/node_modules/vite-plugin-checker/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-plugin-checker/bin/vls" "$@"
else
  exec node  "$basedir/../vite-plugin-checker/bin/vls" "$@"
fi
